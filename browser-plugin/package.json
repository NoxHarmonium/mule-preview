{
  "name": "mule-preview-browser-plugin",
  "version": "1.0.0",
  "description": "Overlays the Mule Preview plugin over SCM web applications",
  "license": "MIT",
  "devDependencies": {
    "addons-linter": "^1.10.1",
    "babel-core": "^6.26.0",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^3.1.0",
    "eslint": "^6.0.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^4.1.0",
    "node-sass": "^4.6.0",
    "prettier": "^1.18.2",
    "rimraf": "^2.6.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "url-loader": "^2.1.0",
    "web-ext": "^3.1.0",
    "webpack": "^4.36.1",
    "webpack-cli": "^3.3.6"
  },
  "scripts": {
    "prebuild": "rimraf ./extension/dist/",
    "start": "webpack --config webpack.config.js -w -d",
    "build": "webpack --config webpack.production.config.js -p",
    "lint": "eslint --fix src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/NoxHarmonium/mule-preview"
  },
  "keywords": [
    "extension",
    "extension-boilerplate",
    "chrome-extension",
    "firefox-addon"
  ],
  "bugs": {
    "url": "https://github.com/NoxHarmonium/mule-preview/issues"
  },
  "homepage": "https://github.com/NoxHarmonium/mule-preview",
  "standard": {
    "globals": [
      "browser",
      "chrome",
      "window"
    ],
    "ignore": [
      "extension/dist/",
      "node_modules/"
    ]
  },
  "dependencies": {
    "cross-fetch": "^3.0.4",
    "react": "16.3.2",
    "webextension-polyfill": "^0.4.0"
  },
  "engines": {
    "node": ">= 10.0.0"
  },
  "browserslist": "defaults"
}
